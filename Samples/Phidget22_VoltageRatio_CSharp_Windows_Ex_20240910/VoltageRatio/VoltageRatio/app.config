<?xml version="1.0" encoding="utf-8"?>
<configuration>
<configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
        <section name="VoltageRatio_Example.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>

</configSections>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/></startup>
    <applicationSettings>
        <VoltageRatio_Example.Properties.Settings>
            <setting name="calibrationURL" serializeAs="String">
                <value>https://www.phidgets.com/docs/Calibrating_Load_Cells</value>
            </setting>
            <setting name="title0" serializeAs="String">
                <value>Step 1 - Zero Load</value>
            </setting>
            <setting name="title1" serializeAs="String">
                <value>Step 2a - Rated Output</value>
            </setting>
            <setting name="state0" serializeAs="String">
                <value>1. Remove all external weight from the system.

2. Wait until the measurement is stable and press "Continue".</value>
            </setting>
            <setting name="state1" serializeAs="String">
                <value>1. If you have a calibration sheet for your load cell(s), enter the rated output and the load cell capacity below.  Then press "Calibrate".

2. If you do not have a calibration sheet for your load cell(s), press "Skip".</value>
            </setting>
            <setting name="state2" serializeAs="String">
                <value>1. Apply a known weight to the system.  This should be as close to the maximum rating for the system as possible.

2. Enter the weight into the input box below.

3. Wait until the measurement is stable and press "Calibrate".</value>
            </setting>
            <setting name="title2" serializeAs="String">
                <value>Step 2b - Non-Zero Load</value>
            </setting>
            <setting name="ratedOutputURL" serializeAs="String">
                <value>https://www.phidgets.com/docs/Calibrating_Load_Cells#Step_2a:_Enter_the_Rated_Output</value>
            </setting>
        </VoltageRatio_Example.Properties.Settings>
    </applicationSettings>

</configuration>
